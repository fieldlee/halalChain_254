version: '2'

networks:
  halal:
services:
  ca0:
    image: hyperledger/fabric-ca
    environment:
      - FABRIC_CA_HOME=/etc/hyperledger/fabric-ca-server
      - FABRIC_CA_SERVER_CA_NAME=ca-org1
      - FABRIC_CA_SERVER_TLS_ENABLED=true
      - FABRIC_CA_SERVER_TLS_CERTFILE=/etc/hyperledger/fabric-ca-server-config/ca.ageli.com-cert.pem
      - FABRIC_CA_SERVER_TLS_KEYFILE=/etc/hyperledger/fabric-ca-server-config/5c06ab7a0cb05d2b9df9bdffd7ba44ce4ed205538faf6ceddad475909747e2a9_sk
    ports:
      - "7054:7054"
    command: sh -c 'GODEBUG=netdns=go fabric-ca-server start --ca.certfile /etc/hyperledger/fabric-ca-server-config/ca.ageli.com-cert.pem --ca.keyfile /etc/hyperledger/fabric-ca-server-config/5c06ab7a0cb05d2b9df9bdffd7ba44ce4ed205538faf6ceddad475909747e2a9_sk -b admin:adminpw -d'
    volumes:
      - ./crypto-config/peerOrganizations/ageli.com/ca/:/etc/hyperledger/fabric-ca-server-config
      - ./channel-artifacts/fabric-ca-server-config.yaml:/etc/hyperledger/fabric-ca-server/fabric-ca-server-config.yaml
    container_name: ca_peerOrg1
    networks:
      - halal

  ca1:
    image: hyperledger/fabric-ca
    environment:
      - FABRIC_CA_HOME=/etc/hyperledger/fabric-ca-server
      - FABRIC_CA_SERVER_CA_NAME=ca-org2
      - FABRIC_CA_SERVER_TLS_ENABLED=true
      - FABRIC_CA_SERVER_TLS_CERTFILE=/etc/hyperledger/fabric-ca-server-config/ca.creator.com-cert.pem
      - FABRIC_CA_SERVER_TLS_KEYFILE=/etc/hyperledger/fabric-ca-server-config/c9c7e11bde41ef312b300b340d051bab6295a05ea7996ac461c408e5102c671f_sk
    ports:
      - "8054:7054"
    command: sh -c 'GODEBUG=netdns=go fabric-ca-server start --ca.certfile /etc/hyperledger/fabric-ca-server-config/ca.creator.com-cert.pem --ca.keyfile /etc/hyperledger/fabric-ca-server-config/c9c7e11bde41ef312b300b340d051bab6295a05ea7996ac461c408e5102c671f_sk -b admin:adminpw -d'
    volumes:
      - ./crypto-config/peerOrganizations/creator.com/ca/:/etc/hyperledger/fabric-ca-server-config
      - ./channel-artifacts/fabric-ca-server-config.yaml:/etc/hyperledger/fabric-ca-server/fabric-ca-server-config.yaml
    container_name: ca_peerOrg2
    networks:
      - halal

  ca2:
    image: hyperledger/fabric-ca
    environment:
      - FABRIC_CA_HOME=/etc/hyperledger/fabric-ca-server
      - FABRIC_CA_SERVER_CA_NAME=ca-org3
      - FABRIC_CA_SERVER_TLS_ENABLED=true
      - FABRIC_CA_SERVER_TLS_CERTFILE=/etc/hyperledger/fabric-ca-server-config/ca.transfer.com-cert.pem
      - FABRIC_CA_SERVER_TLS_KEYFILE=/etc/hyperledger/fabric-ca-server-config/7849bfdf5eaa0b61bb9a9a8e00a7b72ccfc82e2de2c0e56ba4f316516bb7e94e_sk
    ports:
      - "9054:7054"
    command: sh -c 'GODEBUG=netdns=go fabric-ca-server start --ca.certfile /etc/hyperledger/fabric-ca-server-config/ca.transfer.com-cert.pem --ca.keyfile /etc/hyperledger/fabric-ca-server-config/7849bfdf5eaa0b61bb9a9a8e00a7b72ccfc82e2de2c0e56ba4f316516bb7e94e_sk -b admin:adminpw -d'
    volumes:
      - ./crypto-config/peerOrganizations/transfer.com/ca/:/etc/hyperledger/fabric-ca-server-config
      - ./channel-artifacts/fabric-ca-server-config.yaml:/etc/hyperledger/fabric-ca-server/fabric-ca-server-config.yaml
    container_name: ca_peerOrg3
    networks:
      - halal

  ca3:
    image: hyperledger/fabric-ca
    environment:
      - FABRIC_CA_HOME=/etc/hyperledger/fabric-ca-server
      - FABRIC_CA_SERVER_CA_NAME=ca-org4
      - FABRIC_CA_SERVER_TLS_ENABLED=true
      - FABRIC_CA_SERVER_TLS_CERTFILE=/etc/hyperledger/fabric-ca-server-config/ca.seller.com-cert.pem
      - FABRIC_CA_SERVER_TLS_KEYFILE=/etc/hyperledger/fabric-ca-server-config/CA4_PRIVATE_KEY
    ports:
      - "10054:7054"
    command: sh -c 'GODEBUG=netdns=go fabric-ca-server start --ca.certfile /etc/hyperledger/fabric-ca-server-config/ca.seller.com-cert.pem --ca.keyfile /etc/hyperledger/fabric-ca-server-config/CA4_PRIVATE_KEY -b admin:adminpw -d'
    volumes:
      - ./crypto-config/peerOrganizations/seller.com/ca/:/etc/hyperledger/fabric-ca-server-config
      - ./channel-artifacts/fabric-ca-server-config.yaml:/etc/hyperledger/fabric-ca-server/fabric-ca-server-config.yaml
    container_name: ca_peerOrg4
    networks:
      - halal

  orderer0.ageliorderer.com:
    extends:
      file:   base/docker-compose-base.yaml
      service: orderer0.ageliorderer.com
    container_name: orderer0.ageliorderer.com
    networks:
      - halal

  peer0.ageli.com:
    container_name: peer0.ageli.com
    extends:
      file:  base/docker-compose-base.yaml
      service: peer0.ageli.com
    networks:
      - halal

  peer1.ageli.com:
    container_name: peer1.ageli.com
    extends:
      file:  base/docker-compose-base.yaml
      service: peer1.ageli.com
    networks:
      - halal

  peer0.creator.com:
    container_name: peer0.creator.com
    extends:
      file:  base/docker-compose-base.yaml
      service: peer0.creator.com
    networks:
      - halal

  peer1.creator.com:
    container_name: peer1.creator.com
    extends:
      file:  base/docker-compose-base.yaml
      service: peer1.creator.com
    networks:
      - halal

  peer0.transfer.com:
    container_name: peer0.transfer.com
    extends:
      file:  base/docker-compose-base.yaml
      service: peer0.transfer.com
    networks:
      - halal

  peer1.transfer.com:
    container_name: peer1.transfer.com
    extends:
      file:  base/docker-compose-base.yaml
      service: peer1.transfer.com
    networks:
      - halal

  peer0.seller.com:
    container_name: peer0.seller.com
    extends:
      file:  base/docker-compose-base.yaml
      service: peer0.seller.com
    networks:
      - halal

  peer1.seller.com:
    container_name: peer1.seller.com
    extends:
      file:  base/docker-compose-base.yaml
      service: peer1.seller.com
    networks:
      - halal
